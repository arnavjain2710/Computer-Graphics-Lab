   "source": [
    "def midpoint(x1, y1, x2, y2):\n",
    "    Mx = (x1 + x2) / 2\n",
    "    My = (y1 + y2) / 2\n",
    "    return Mx, My\n",
    "\n",
    "x1, y1 = 2, 3\n",
    "x2, y2 = 6, 7\n",
    "Mx, My = midpoint(x1, y1, x2, y2)\n",
    "\n",
    "print(f\"Midpoint between A({x1}, {y1}) and B({x2}, {y2}) is M({Mx}, {My})\")"
   ]
